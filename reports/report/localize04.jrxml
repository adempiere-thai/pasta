	<subDataset name="WithholdingTaxDetail">
		<parameter name="p_c_whtaxtrans_id" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select wtl.datetrx ,wtl.remarks as description  , w.percent , wtl.taxbaseamt , wtl.taxamt , wt.whtaxtype
from c_whtaxtransline wtl
inner join c_whtaxtrans wt on wt.c_whtaxtrans_id = wtl.c_whtaxtrans_id
inner join c_withholding w on wtl.c_withholding_id = w.c_withholding_id
where wtl.c_whtaxtrans_id = $P{p_c_whtaxtrans_id}
order by wtl.c_whtaxtrans_id , wtl.c_revenuetype_id]]>
		</queryString>
		<field name="datetrx" class="java.sql.Timestamp"/>
		<field name="description" class="java.lang.String"/>
		<field name="percent" class="java.math.BigDecimal"/>
		<field name="taxbaseamt" class="java.math.BigDecimal"/>
		<field name="taxamt" class="java.math.BigDecimal"/>
		<field name="whtaxtype" class="java.lang.String"/>
		<variable name="v_taxbase_decimal" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_C_Period_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_Client_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_Org_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select t.* from (
select oi.taxid as withholder_taxid , oi.branchno as withholder_branchno, wht.seqno,coalesce(wht.taxid,wht.citizenid) taxpayer_taxid , wht.branchno as taxpayer_branchno
	,g.greeting , bp.name , bp.name2 ,l.address1 ,l.address2 ,l.address3 ,l.address4 , l.city , l.postal , wht.c_whtaxtrans_id,sum(wtl.taxbaseamt) as taxbaseamt , sum(wtl.taxamt) as taxamt
from c_whtaxtrans wht
inner join c_whtaxtransline wtl on wht.c_whtaxtrans_id = wtl.c_whtaxtrans_id
inner join c_bpartner bp on bp.c_bpartner_id = wht.c_bpartner_id
inner join c_bpartner_location bpl on bpl.c_bpartner_location_id = wht.c_bpartner_location_id
inner join c_location l on bpl.c_location_id = l.c_location_id
inner join c_period p on p.startdate <=wht.datedoc and p.enddate >= wht.datedoc and p.ad_client_id =wht.ad_client_id
left join c_greeting g on g.c_greeting_id = bp.c_greeting_id
inner join ad_orginfo oi on oi.ad_org_id = wht.ad_org_id
where p.c_period_id = $P{p_C_Period_ID}
and wht.AD_Client_ID = $P{AD_Client_ID}
and wht.AD_Org_ID = COALESCE($P{AD_Org_ID},wht.AD_Org_ID)
and wht.pndtype = '3'
group by oi.taxid , oi.branchno , wht.seqno,coalesce(wht.taxid,wht.citizenid)  , wht.branchno
	,g.greeting , bp.name , bp.name2 ,l.address1 ,l.address2 ,l.address3 ,l.address4 , l.city , l.postal , wht.c_whtaxtrans_id
) t order by t.seqno]]>
	</queryString>
